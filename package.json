{
  "name": "yet-another-react-bootstrap",
  "version": "0.0.2",
  "description": "React bootstrap application. React, webpack, ES6/7, eslint, karma, mocha, chai, sinon, enzyme, browsersync.",
  "private": true,
  "engines": {
    "node": ">=5.0 <7",
    "npm": ">=3.3 <4"
  },
  "main": "",
  "scripts": {
    "eslint": "eslint --ext .js,.jsx src",
    "stylelint": "stylelint src/**/*.css",
    "lint": "npm run eslint && npm run stylelint",
    "clean": "rimraf build",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.prod.babel.js --progress -p",
    "start": "npm run clean && NODE_ENV=development webpack-dev-server --config webpack.dev.babel.js --hot --inline --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aeldar/yet-another-react-bootstrap.git"
  },
  "keywords": [
    "react",
    "webpack",
    "bootstrap"
  ],
  "author": "Eldar Aliyev <eldar.aliyev8@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aeldar/yet-another-react-bootstrap/issues"
  },
  "homepage": "https://github.com/aeldar/yet-another-react-bootstrap#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.0",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.3.0",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.19.0",
    "json-loader": "^0.5.4",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "pug": "^2.0.0-beta2",
    "pug-html-loader": "^1.0.5",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "stylelint": "^6.5.1",
    "stylelint-config-standard": "^8.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "normalize.css": "^4.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1"
  }
}
